@inject Microsoft.AspNetCore.Mvc.Localization.IViewLocalizer localizer 
<!DOCTYPE html>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<title>Job Hunt</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta name="description" content="">
	<meta name="keywords" content="">
	<meta name="author" content="CreativeLayers">

	<!-- Styles -->
	<link rel="stylesheet" type="text/css" href="~/adminJob/css/bootstrap-grid.css" />
	<link rel="stylesheet" href="~/adminJob/css/icons.css">
	<link rel="stylesheet" href="~/adminJob/css/animate.min.css">
	<link rel="stylesheet" type="text/css" href="~/adminJob/css/style.css" />
	<link rel="stylesheet" type="text/css" href="~/adminJob/css/responsive.css" />
	<link rel="stylesheet" type="text/css" href="~/adminJob/css/chosen.css" />
	<link rel="stylesheet" type="text/css" href="~/adminJob/css/colors/colors.css" />
	<link rel="stylesheet" type="text/css" href="~/adminJob/css/bootstrap.css" />
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css" />
	
</head>
<body>

<div class="page-loading">
	<img src="images/loader.gif" alt="" />
	<span>Skip Loader</span>
</div>

<div class="theme-layout" id="scrollup">
	
	<div class="responsive-header">
		<div class="responsive-menubar">
			<div class="res-logo"><a href="index.html" title=""><img src="~/adminJob/images/resource/logo.png" alt="" /></a></div>
			<div class="menu-resaction">
				<div class="res-openmenu">
					<img src="images/icon.png" alt="" /> Menu
				</div>
				<div class="res-closemenu">
					<img src="images/icon2.png" alt="" /> Close
				</div>
			</div>
		</div>
		<div class="responsive-opensec">
			<div class="btn-extars">
				<a href="job_list_grid.html#" title="" class="post-job-btn">Post Jobs</a>
				<ul class="account-btns">
					<li class="signup-popup"><a title="">Sign Up</a></li>
					<li class="signin-popup"><a title="">Login</a></li>
				</ul>
			</div><!-- Btn Extras -->
			<form class="res-search">
				<input type="text" placeholder="Job title, keywords or company name" />
				<button type="submit">Search</button>
			</form>
			<div class="responsivemenu">
				<ul>
						<li class="menu-item-has-children">
							<a href="job_list_grid.html#" title="">Employers</a>
							<ul>
										<li><a href="employer_manage_jobs.html" title="">Employer Job Manager</a></li>
										<li><a href="employer_packages.html" title="">Employer Packages</a></li>
										<li><a href="employer_post_new.html" title="">Employer Post New</a></li>
										<li><a href="employer_profile.html" title="">Employer Profile</a></li>
										<li><a href="employer_resume.html" title="">Employer Resume</a></li>
										<li><a href="employer_transactions.html" title="">Employer Transaction</a></li>
										<li><a href="employer_job_alert.html" title="">Employer Job Alert</a></li>
										<li><a href="employer_change_password.html" title="">Employer Change Password</a></li>

							</ul>
						</li>
					</ul>
			</div>
		</div>
	</div>
	
	<header class="stick-top">
		<div class="menu-sec">
			<div class="container">
				<div class="logo">
					<a href="index.html" title=""><img src="~/adminJob/images/resource/logo.png" alt="" /></a>
				</div><!-- Logo -->
				<div class="btn-extars">
					<ul class="account-btns">
						<li  class="signup-popup"><a asp-action="Login" asp-controller="Admin" title="">@localizer["Admin"]</a></li>
						<li  class="signup-popup"><a asp-action="Register" asp-controller="Job" title="">@localizer["Employer Sign Up"]</a></li>
						<li class="signin-popup"><a asp-action="Login1" asp-controller="User" title="">@localizer["Employer Login"]</a></li>
		                <li class="nav-item dropdown"> <partial name="_Culture" /> </li>
						<li class="signin-popup"> <img src="~/Images/United.png" alt="" /></li>
					</ul>
				</div><!-- Btn Extras -->

			</div>
		</div>
	</header>

	<section class="overlape">
		<div class="block no-padding">
			<div data-velocity="-.1" style="background: url(adminJob/images/resource/mslider1.jpg) repeat scroll 50% 422.28px transparent;" class="parallax scrolly-invisible no-parallax"></div><!-- PARALLAX BACKGROUND IMAGE -->
			<div class="container fluid">
				<div class="row">
					<div class="col-lg-12">
						<div class="inner-header wform">
							<div class="job-search-sec">
								<div class="job-search">
									<h4>@localizer["Explore Thousand Of Jobs With Just Simple Search..."]</h4>
									
										<form asp-action="List" asp-controller="Job" method="get" class="mx-auto">
										<div class="row">
											<div class="col-lg-9">
												<div class="job-field">
													<input type="text" name="SearchString" value="@ViewData["currentFilter"]" placeholder="@localizer["Job title or Salary"]" />
												</div>
											</div>
											<div class="col-lg-2">
												<button  value="Search" type="submit">@localizer["Search"]</button>
											</div>
										</div>
									</form>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>

	<div class="container">
         @RenderBody()
    </div>

	<footer>
		<div class="block">
			<div class="container">
				<div class="row">
					<div class="col-lg-3 column">
						<div class="widget">
							<div class="about_widget">
								<div class="logo">
									<a asp-action="app404" asp-controller="App" title=""><img src="~/adminJob/images/resource/logo.png" alt="" /></a>
								</div>
								<span>Uzbekistan</span>
								<span>+99893 123 45 67</span>
								<span>info@jobhunt.com</span>
								<div class="social">
									<a href="job_list_grid.html#" title=""><i class="fa fa-facebook"></i></a>
									<a href="job_list_grid.html#" title=""><i class="fa fa-twitter"></i></a>
									<a href="job_list_grid.html#" title=""><i class="fa fa-linkedin"></i></a>
									<a href="job_list_grid.html#" title=""><i class="fa fa-pinterest"></i></a>
									<a href="job_list_grid.html#" title=""><i class="fa fa-behance"></i></a>
								</div>
							</div><!-- About Widget -->
						</div>
					</div>
					<div class="col-lg-4 column">
						<div class="widget">
							<h3 class="footer-title">Frequently Asked Questions</h3>
							<div class="link_widgets">
								<div class="row">
									<div class="col-lg-6">
										<a href="job_list_grid.html#" title="">Privacy & Seurty </a>
										<a href="job_list_grid.html#" title="">Terms of Serice</a>
										<a href="job_list_grid.html#" title="">Communications </a>
										<a href="job_list_grid.html#" title="">Referral Terms </a>
										<a href="job_list_grid.html#" title="">Lending Licnses </a>
										<a href="job_list_grid.html#" title="">Disclaimers </a>	
									</div>
									<div class="col-lg-6">
										<a href="job_list_grid.html#" title="">Support </a>
										<a href="job_list_grid.html#" title="">How It Works </a>
										<a href="job_list_grid.html#" title="">For Employers </a>
										<a href="job_list_grid.html#" title="">Underwriting </a>
										<a href="job_list_grid.html#" title="">Contact Us</a>
									</div>
								</div>
							</div>
						</div>
					</div>
					<div class="col-lg-2 column">
						<div class="widget">
							<h3 class="footer-title">Find Jobs</h3>
							<div class="link_widgets">
								<div class="row">
									<div class="col-lg-12">
										<a href="job_list_grid.html#" title="">US Jobs</a>	
										<a href="job_list_grid.html#" title="">Canada Jobs</a>	
										<a href="job_list_grid.html#" title="">UK Jobs</a>	
										<a href="job_list_grid.html#" title="">Emplois en Fnce</a>	
										<a href="job_list_grid.html#" title="">Jobs in Deuts</a>	
										<a href="job_list_grid.html#" title="">Vacatures China</a>	
									</div>
								</div>
							</div>
						</div>
					</div>
					<div class="col-lg-3 column">
						<div class="widget">
							<div class="download_widget">
								<a href="job_list_grid.html#" title=""><img src="~/adminJob/images/resource/dw1.png" alt="" /></a>
								<a href="job_list_grid.html#" title=""><img src="~/adminJob/images/resource/dw2.png" alt="" /></a>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="bottom-line">
			<span>© 2022 Jobhunt All rights reserved. ATMDT-2018</span>
			<a href="job_list_grid.html#scrollup" class="scrollup" title=""><i class="la la-arrow-up"></i></a>
		</div>
	</footer>

</div>

<script src="~/adminJob/js/jquery.min.js" type="text/javascript"></script>
<script src="~/adminJob/js/modernizr.js" type="text/javascript"></script>
<script src="~/adminJob/js/script.js" type="text/javascript"></script>
<script src="~/adminJob/js/bootstrap.min.js" type="text/javascript"></script>
<script src="~/adminJob/js/wow.min.js" type="text/javascript"></script>
<script src="~/adminJob/js/slick.min.js" type="text/javascript"></script>
<script src="~/adminJob/js/parallax.js" type="text/javascript"></script>
<script src="~/adminJob/js/select-chosen.js" type="text/javascript"></script>

<!--Create start-->
<script src="~/adminJob/js/jquery.scrollbar.min.js" type="text/javascript"></script>
<script src="~/adminJob/js/tag.js" type="text/javascript"></script>
<script src="~/adminJob/js/maps3.js" type="text/javascript"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAAz77U5XQuEME6TpftaMdX0bBelQxXRlM&callback=initMap"type="text/javascript"></script>

</body>
